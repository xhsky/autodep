{
  "mq":{
    "software": ["rabbitmq"], 
    "located": "/data/", 
    "rabbitmq_info":{
      "cluster_name": "mq_cluster",             // 集群名称, 需相同
      "node_type": "disc",                      // 节点类型, 第一个写的mq节点必须为disc
      "members": ["mq", "mq1","mq2"],           // 集群成员
      "vhosts": ["/vhost"],                     // 虚拟机名称
      "users": ["a1"],                          // 用户
      "passwords": ["111111"]                   // 密码, vhost-user-password 一一对应
     }
  }, 
  "mq1":{
    "software": ["rabbitmq"], 
    "located": "/data/", 
    "rabbitmq_info":{
      "cluster_name": "mq_cluster", 
      "node_type": "ram", 
      "members": ["mq", "mq1","mq2"]
     }
  },
  "mq2":{
    "software": ["rabbitmq"], 
    "located": "/data/", 
    "rabbitmq_info":{
      "cluster_name": "mq_cluster", 
      "node_type": "ram", 
      "members": ["mq", "mq1","mq2"]
     }
  }
}

